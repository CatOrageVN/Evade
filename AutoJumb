-- AutoJumb | By MeoCamüçä

local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local autoJump = false

-- GUI
local gui = Instance.new("ScreenGui")
gui.Name = "AutoJumbGUI"
gui.Parent = player:WaitForChild("PlayerGui")

local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 260, 0, 150)
frame.Position = UDim2.new(0.5, -130, 0.5, -75)
frame.BackgroundColor3 = Color3.fromRGB(255, 140, 0) -- CAM
frame.Active = true
frame.Draggable = true
frame.Parent = gui

local inside = Instance.new("Frame")
inside.Size = UDim2.new(1, -10, 1, -10)
inside.Position = UDim2.new(0, 5, 0, 5)
inside.BackgroundColor3 = Color3.fromRGB(255, 230, 120) -- V√ÄNG
inside.Parent = frame

local title = Instance.new("TextLabel")
title.Size = UDim2.new(1, 0, 0, 35)
title.BackgroundTransparency = 1
title.Text = "AutoJumb - By MeoCamüçä"
title.TextScaled = true
title.Font = Enum.Font.GothamBold
title.TextColor3 = Color3.fromRGB(0,0,0)
title.Parent = inside

local toggleBtn = Instance.new("TextButton")
toggleBtn.Size = UDim2.new(0.7, 0, 0, 40)
toggleBtn.Position = UDim2.new(0.15, 0, 0.55, 0)
toggleBtn.BackgroundColor3 = Color3.fromRGB(0, 200, 0) -- XANH L√Å
toggleBtn.Text = "OFF"
toggleBtn.TextScaled = true
toggleBtn.Font = Enum.Font.GothamBold
toggleBtn.TextColor3 = Color3.fromRGB(255,255,255)
toggleBtn.Parent = inside

-- Function b·∫≠t/t·∫Øt
local function setState(state)
	autoJump = state
	toggleBtn.Text = state and "ON" or "OFF"
	toggleBtn.BackgroundColor3 = state 
		and Color3.fromRGB(0, 200, 0)
		or Color3.fromRGB(200, 0, 0)
end

toggleBtn.MouseButton1Click:Connect(function()
	setState(not autoJump)
end)

-- Ph√≠m E b·∫≠t t·∫Øt
UserInputService.InputBegan:Connect(function(input, gpe)
	if gpe then return end
	if input.KeyCode == Enum.KeyCode.E then
		setState(not autoJump)
	end
end)

-- Auto Jump
RunService.Heartbeat:Connect(function()
	if autoJump then
		local char = player.Character
		if char and char:FindFirstChild("Humanoid") then
			char.Humanoid.Jump = true
		end
	end
end)
